{
  "redirects": [
    {
      "source": "/privacidad.html",
      "destination": "/privacidad",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/404",
      "destination": "/404.html"
    }
  ],
  "rewrites": [
    {
      "source": "/privacidad",
      "destination": "/privacidad.html"
    },
    {
      "source": "/privacidad/",
      "destination": "/privacidad.html"
    },
    {
      "source": "/contacto",
      "destination": "/contacto.html"
    },
    {
      "source": "/contacto/",
      "destination": "/contacto.html"
    },
    {
      "source": "/terminos",
      "destination": "/terminos.html"
    },
    {
      "source": "/terminos/",
      "destination": "/terminos.html"
    },
    {
      "source": "/sobre-nosotros",
      "destination": "/sobre-nosotros.html"
    },
    {
      "source": "/sobre-nosotros/",
      "destination": "/sobre-nosotros.html"
    }
  ],
  "redirects": [
    {
      "source": "/contacto.html",
      "destination": "/contacto",
      "permanent": true
    },
    {
      "source": "/terminos.html",
      "destination": "/terminos",
      "permanent": true
    },
    {
      "source": "/sobre-nosotros.html",
      "destination": "/sobre-nosotros",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

